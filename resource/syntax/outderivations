AssignOp -> ValidTokenType(Assign)
match Assign
Expr -> ArithExpr ExprTail
ArithExpr -> Term RightRecArithExpr
Term -> Factor RightRecTerm
Factor -> FuncCallOrVar
FuncCallOrVar -> ValidTokenType(Id) FuncCallOrVarTail ReptFuncCallOrVar2
match Id
FuncCallOrVarTail -> Indices
Indices -> ReptIndices0
ReptIndices0 -> EPSILON
ReptFuncCallOrVar2 -> MemberCall ReptFuncCallOrVar2
MemberCall -> ValidTokenType(Dot) ValidTokenType(Id) FuncCallOrVarTail
match Dot
match Id
FuncCallOrVarTail -> Indices
Indices -> ReptIndices0
ReptIndices0 -> Indice ReptIndices0
